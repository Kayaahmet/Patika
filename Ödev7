Soru1
SELECT rating 
FROM film
GROUP BY rating;

Soru2
SELECT replacement_cost,COUNT(*) 
FROM film
GROUP BY replacement_cost
HAVING COUNT(*) > 50;

Soru3
SELECT store_id, COUNT(*) 
FROM customer
GROUP BY store_id;

Soru4
SELECT country_id, COUNT(city) 
FROM city
GROUP BY country_id
ORDER BY COUNT DESC;
